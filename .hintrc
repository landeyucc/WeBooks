{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-wrap",
          "text-wrap: balance",
          "scrollbar-width",
          "-ms-overflow-style"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "definition-list": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/html": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "meta-viewport": "off"
  }
}